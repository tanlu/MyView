<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--自定义属性
        name 属性名  format 属性类型

        声明样式
        declarestyle
    -->
    <attr name="textContent" format="string"></attr>
    <attr name="textColor" format="color"></attr>
    <attr name="textSize" format="dimension"></attr>

    <declare-styleable name="MyThem">
        <attr name="textContent"></attr>
        <attr name="textColor"></attr>
        <attr name="textSize"></attr>
    </declare-styleable>
</resources>
